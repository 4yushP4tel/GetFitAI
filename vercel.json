{
    "version": 2,
    "builds": [
        {
            "src": "fitness.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/home",
            "dest": "/home"
        },
        {
            "src": "/(.*)",
            "dest": "/"

        }
    ]
}